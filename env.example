# Example environment variables for the project.
# Copy this file to `.env` and edit values as needed: `cp env.example .env`

# MySQL / Prisma
DATABASE_URL="mysql://user:password@tcp(db:3306)/imagedb"

# Backend
PORT=3000
AI_API_KEY=
AI_MODEL=
# AI_BASE_URL 可选：留空表示使用 litellm 默认 base url（或禁用自定义 base url）；如果不打算启用 AI 功能，也可以不填写或删除相关 AI 变量
AI_BASE_URL=

# Frontend build-time API base (used as build-arg VITE_API_BASE_URL)
VITE_API_BASE_URL="http://backend:3000/api"

# Optional overrides for compose users (not required if using DATABASE_URL)
MYSQL_ROOT_PASSWORD=example
MYSQL_DATABASE=imagedb
MYSQL_USER=user
MYSQL_PASSWORD=password


